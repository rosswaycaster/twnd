{
  "name": "twnd",
  "description": "A simple utility for compiling a readable list of tailwindcss classes",
  "version": "0.0.12",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "css",
    "classes",
    "utility",
    "classname",
    "classnames",
    "readable",
    "manage",
    "list",
    "compose",
    "react",
    "reactjs",
    "node",
    "javascript",
    "vue",
    "vuejs",
    "twnd",
    "twd",
    "tnwd",
    "tailwnd",
    "tlnd",
    "tlwnd"
  ],
  "scripts": {
    "build": "pika build",
    "release": "pika build && cd pkg && npm login && npm publish",
    "test": "jest ./src"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rosswaycaster/twnd.git"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-umd",
        {
          "name": "twnd"
        }
      ]
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-umd": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^25.4.0",
    "jest": "^25.4.0"
  }
}
